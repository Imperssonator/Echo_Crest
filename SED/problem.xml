<?xml version="1.0" encoding="ISO-8859-1" ?>
<Problem>

	<!-- Specify the input dimensions here. This is a 2D example. -->
	<Inputs>
	
		<!-- The first input works in the range [-1,3] and is twice as important as the second parameter. -->
		<Input name="x" minimum="-1" maximum="3" weight="2.0"/>
		
		<!-- If you leave out some attributes, they are set to their default values (minimum = -1, maximum = 1, weight = 1). -->
		<Input name="y"/>
		
		<!-- Define a constraint. myMatlabFunction must be a function file on the Matlab path. see myMatlabFunction.m for an example file. -->
		<Constraint>
			<Option key="file" value="problemConstraint"/>
		</Constraint>
		
	</Inputs>
	
	<!-- Outputs are strictly optional. You can remove these if you don't need them. -->
	<Outputs>
		<Output name="out"/>
	</Outputs>
	
</Problem>